<?xml version="1.0"?>
<!DOCTYPE StGermainData SYSTEM "stgermain.dtd">

<StGermainData xmlns="http://www.vpac.org/StGermain/XML_IO_Handler/Jun2003">

	<struct name="components" mergeType="merge">
	<!--
		<struct name="camera">
			<param name="Type">lucCamera</param>
			<param name="CentreFieldVariable">VelocityMagnitudeField</param>
			<param name="coordZ">1.5</param>
		</struct>
		-->
		<struct name="velocityMagnitudeColourMap" mergeType="replace">
			<param name="Type">lucColourMap</param>
			<param name="colours">Purple Blue Green Yellow Orange Red</param>
			<param name="dynamicRange">true</param>
		</struct>
		<struct name="timeStep" mergeType="replace">
			<param name="Type">lucTimeStep</param>
			<param name="colour">Black</param>
			<param name="frame">false</param>
			<param name="time">true</param>
		</struct>

		<!-- Drawing Objects -->

		<struct name="velocityMagnitudeTitle" mergeType="replace">
			<param name="Type">lucTitle</param>
			<param name="string">Velocity Magnitude</param>
		</struct>
		<struct name="velocityMagnitudeMap">
			<param name="Type">lucScalarFieldOnMesh</param>
			<param name="FieldVariable">VelocityMagnitudeField</param>
			<param name="ColourMap">velocityMagnitudeColourMap</param>
			<param name="resolution">128</param>
		</struct>
		<struct name="pressureColourMap">
			<param name="Type">lucColourMap</param>
			<param name="colours">Blue Red</param>
			<param name="dynamicRange">true</param>
			<!--
			<param name="maximum">3</param>
			<param name="minimum">-2.1</param>
			-->
		</struct>
		<struct name="pressureColourBar" mergeType="replace">
			<param name="Type">lucColourBar</param>
			<param name="ColourMap">pressureColourMap</param>
		</struct>
		 <struct name="pressureMap" mergeType="replace">
        <param name="Type">lucMeshViewer</param>
        <param name="Mesh">pressureMesh</param>
        <param name="nodeNumbers">False</param>
        <param name="elementNumbers">False</param>
        <param name="displayNodes">True</param>
        <param name="displayEdges">False</param>
        <param name="pointSmoothing">True</param>
        <param name="pointSize">15.0</param>
        <param name="ColourMap">pressureColourMap</param>
        <param name="ColourField">PressureField</param>
    </struct>

			<!--
		<struct name="pressureMap">
			<param name="Type">lucScalarFieldOnMesh</param>
			<param name="FieldVariable">NodalPressureField</param>
			<param name="ColourMap">pressureColourMap</param>
			<param name="resolution">128</param>
		</struct>
			-->
		<struct name="velocityArrows" mergeType="replace">
			<param name="Type">lucVectorArrowsOnMesh</param>
			<param name="VectorVariable">VelocityField</param>
			<param name="Colour">black</param>
			<param name="arrowHeadSize">0.15</param>	
			<param name="lengthScale">0.15</param>	
		</struct>
		<struct name="border" mergeType="replace">
			<param name="Type">lucFieldVariableBorder</param>
			<param name="FieldVariable">VelocityMagnitudeField</param>
		</struct>
		<struct name="velocityMagnitudeColourBar" mergeType="replace">
			<param name="Type">lucColourBar</param>
			<param name="ColourMap">velocityMagnitudeColourMap</param>
		</struct>
		<!--
		<struct name="particleMaterialDots">
			<param name="Type">lucSwarmViewer</param>
			<param name="Swarm">materialSwarm</param>
			<param name="ColourVariable">materialSwarm-MaterialIndex</param>
			<param name="ColourMap">materialColourMap</param>
			<param name="pointSize">5.0</param>
		</struct>
		<struct name="materialColourMap">
			<param name="Type">lucColourMap</param>
			<param name="colours">Red Yellow Green</param>
			<param name="dynamicRange">False</param>
			<param name="minimum">0</param>
			<param name="maximum">2</param>
		</struct>
				<param>particleMaterialDots</param>
				-->
		<!-- Put these althogether in a viewport -->
		<struct name="VelocityVP" mergeType="replace">
			<param name="Type">lucViewport</param>
			<param name="Camera">camera</param>
			<list name="DrawingObject">
				<param>velocityMagnitudeTitle</param>
				<param>velocityMagnitudeMap</param>
				<!--
				<param>pressureMap</param>
				-->
				<param>timeStep</param>
				<param>border</param>
				<param>velocityArrows</param>
				<param>velocityMagnitudeColourBar</param>
			</list>
		</struct>
		<struct name="PressureVP" mergeType="replace">
			<param name="Type">lucViewport</param>
			<param name="Camera">camera</param>
			<list name="DrawingObject">
				<param>pressureMap</param>
				<param>timeStep</param>
				<param>border</param>
				<param>velocityArrows</param>
				<param>pressureColourBar</param>
			</list>
		</struct>
	</struct>
</StGermainData>
