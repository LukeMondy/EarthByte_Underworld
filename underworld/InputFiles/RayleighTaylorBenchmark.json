{
   "import": [
      "Underworld", 
      "Underworld", 
      "gLucifer"
   ], 
   "plugins": [
      {
         "Type": "StgFEM_StandardConditionFunctions", 
         "Context": "context"
      }, 
      {
         "Type": "StgFEM_FrequentOutput", 
         "Context": "context"
      }, 
      {
         "Type": "Underworld_Vrms", 
         "Context": "context", 
         "GaussSwarm": "gaussSwarm", 
         "VelocityField": "VelocityField"
      }, 
      {
         "Type": "StgFEM_CPUTime", 
         "Context": "context"
      }
   ], 
   "components": {
      "constantMesh": {
         "Type": "FeMesh", 
         "elementType": "constant"
      }, 
      "constantMeshGenerator": {
         "Type": "C0Generator", 
         "mesh": "constantMesh", 
         "elementMesh": "elementMesh"
      }, 
      "linearMesh": {
         "Type": "FeMesh", 
         "elementType": "linear"
      }, 
      "linearMeshGenerator": {
         "Type": "CartesianGenerator", 
         "mesh": "linearMesh", 
         "dims": "dim", 
         "shadowDepth": "shadowDepth", 
         "size": [
            "elementResI", 
            "elementResJ", 
            "elementResK"
         ], 
         "minCoord": [
            "minX", 
            "minY", 
            "minZ"
         ], 
         "maxCoord": [
            "maxX", 
            "maxY", 
            "maxZ"
         ]
      }, 
      "velocity": {
         "Type": "MeshVariable", 
         "mesh": "velocityMesh", 
         "Rank": "Vector", 
         "DataType": "Double", 
         "VectorComponentCount": "dim", 
         "names": [
            "vx", 
            "vy", 
            "vz"
         ]
      }, 
      "velocityBCs": {
         "Type": "CompositeVC", 
         "Data": "velocityMesh"
      }, 
      "velocityICs": {
         "Type": "CompositeVC", 
         "Data": "velocityMesh"
      }, 
      "velocityDofLayout": {
         "Type": "DofLayout", 
         "MeshVariable": "velocity"
      }, 
      "VelocityField": {
         "Type": "FeVariable", 
         "FEMesh": "velocityMesh", 
         "DofLayout": "velocityDofLayout", 
         "BC": "velocityBCs", 
         "IC": "velocityICs", 
         "LinkedDofInfo": "velocityLinkedDofs", 
         "outputUnits": "cm/yr"
      }, 
      "VelocityMagnitudeField": {
         "Type": "OperatorFeVariable", 
         "Operator": "Magnitude", 
         "Operand": "VelocityField"
      }, 
      "VelocityGradientsField": {
         "Type": "OperatorFeVariable", 
         "Operator": "Gradient", 
         "Operand": "VelocityField"
      }, 
      "VelocityGradientsInvariantField": {
         "Type": "OperatorFeVariable", 
         "Operator": "TensorInvariant", 
         "Operand": "VelocityGradientsField", 
         "outputUnits": "yr^-1"
      }, 
      "VelocityXXField": {
         "Type": "OperatorFeVariable", 
         "Operator": "TakeFirstComponent", 
         "Operand": "VelocityField", 
         "outputUnits": "cm/yr"
      }, 
      "VelocityYYField": {
         "Type": "OperatorFeVariable", 
         "Operator": "TakeSecondComponent", 
         "Operand": "VelocityField", 
         "outputUnits": "cm/yr"
      }, 
      "StrainRateField": {
         "Type": "OperatorFeVariable", 
         "Operator": "TensorSymmetricPart", 
         "Operand": "VelocityGradientsField", 
         "outputUnits": "yr^-1"
      }, 
      "VorticityField": {
         "Type": "OperatorFeVariable", 
         "Operator": "TensorAntisymmetricPart", 
         "Operand": "VelocityGradientsField", 
         "outputUnits": "yr^-1"
      }, 
      "StrainRateInvariantField": {
         "Type": "OperatorFeVariable", 
         "Operator": "SymmetricTensor_Invariant", 
         "Operand": "StrainRateField", 
         "outputUnits": "yr^-1"
      }, 
      "StrainRateXXField": {
         "Type": "OperatorFeVariable", 
         "Operator": "TakeFirstComponent", 
         "Operand": "StrainRateField", 
         "outputUnits": "yr^-1"
      }, 
      "StrainRateYYField": {
         "Type": "OperatorFeVariable", 
         "Operator": "TakeSecondComponent", 
         "Operand": "StrainRateField", 
         "outputUnits": "yr^-1"
      }, 
      "pressure": {
         "Type": "MeshVariable", 
         "mesh": "pressureMesh", 
         "Rank": "Scalar", 
         "DataType": "Double"
      }, 
      "pressureBCs": {
         "Type": "CompositeVC", 
         "Data": "pressureMesh"
      }, 
      "pressureICs": {
         "Type": "CompositeVC", 
         "Data": "pressureMesh"
      }, 
      "pressureDofLayout": {
         "Type": "DofLayout", 
         "MeshVariable": "pressure"
      }, 
      "PressureField": {
         "Type": "FeVariable", 
         "FEMesh": "pressureMesh", 
         "DofLayout": "pressureDofLayout", 
         "BC": "pressureBCs", 
         "IC": "pressureICs", 
         "LinkedDofInfo": "pressureLinkedDofs", 
         "outputUnits": "GPa"
      }, 
      "cellLayout": {
         "Type": "SingleCellLayout"
      }, 
      "particleLayout": {
         "Type": "GaussParticleLayout"
      }, 
      "gaussSwarm": {
         "Type": "IntegrationPointsSwarm", 
         "CellLayout": "cellLayout", 
         "ParticleLayout": "particleLayout", 
         "FeMesh": "elementMesh", 
         "TimeIntegrator": "timeIntegrator"
      }, 
      "timeIntegrator": {
         "Type": "TimeIntegrator", 
         "order": "timeIntegratorOrder", 
         "simultaneous": "f", 
         "Context": "context"
      }, 
      "elementCellLayout": {
         "Type": "ElementCellLayout", 
         "Mesh": "elementMesh"
      }, 
      "weights": {
         "Type": "PCDVC", 
         "resolutionX": 10, 
         "resolutionY": 10, 
         "resolutionZ": 10, 
         "lowerT": 0.6, 
         "upperT": 25, 
         "maxDeletions": 3, 
         "maxSplits": 3, 
         "MaterialPointsSwarm": "materialSwarm"
      }, 
      "localLayout": {
         "Type": "MappedParticleLayout"
      }, 
      "picIntegrationPoints": {
         "Type": "IntegrationPointsSwarm", 
         "CellLayout": "elementCellLayout", 
         "ParticleLayout": "localLayout", 
         "FeMesh": "elementMesh", 
         "WeightsCalculator": "weights", 
         "TimeIntegrator": "timeIntegrator", 
         "IntegrationPointMapper": "mapper"
      }, 
      "mapper": {
         "Type": "CoincidentMapper", 
         "IntegrationPointsSwarm": "picIntegrationPoints", 
         "MaterialPointsSwarm": "materialSwarm"
      }, 
      "materialSwarmParticleLayout": {
         "Type": "SpaceFillerParticleLayout", 
         "averageInitialParticlesPerCell": "particlesPerCell"
      }, 
      "pMovementHandler": {
         "Type": "ParticleMovementHandler"
      }, 
      "pShadowSync": {
         "Type": "ParticleShadowSync"
      }, 
      "materialSwarm": {
         "Type": "MaterialPointsSwarm", 
         "CellLayout": "elementCellLayout", 
         "ParticleLayout": "materialSwarmParticleLayout", 
         "FeMesh": "elementMesh", 
         "ParticleCommHandlers": [
            "pMovementHandler", 
            "pShadowSync"
         ], 
         "SplittingRoutine": "splittingRoutine", 
         "RemovalRoutine": "removalRoutine", 
         "EscapedRoutine": "escapedRoutine"
      }, 
      "materialSwarmAdvector": {
         "Type": "SwarmAdvector", 
         "Swarm": "materialSwarm", 
         "TimeIntegrator": "timeIntegrator", 
         "VelocityField": "VelocityField", 
         "PeriodicBCsManager": "periodicBCsManager", 
         "allowFallbackToFirstOrder": true
      }, 
      "solutionVelocity": {
         "Type": "SolutionVector", 
         "FeVariable": "VelocityField"
      }, 
      "solutionPressure": {
         "Type": "SolutionVector", 
         "FeVariable": "PressureField"
      }, 
      "mom_force": {
         "Type": "ForceVector", 
         "FeVariable": "VelocityField", 
         "ExtraInfo": "context"
      }, 
      "cont_force": {
         "Type": "ForceVector", 
         "FeVariable": "PressureField", 
         "ExtraInfo": "context"
      }, 
      "k_matrix": {
         "Type": "StiffnessMatrix", 
         "RowVariable": "VelocityField", 
         "ColumnVariable": "VelocityField", 
         "RHS": "mom_force", 
         "allowZeroElementContributions": false
      }, 
      "constitutiveMatrix": {
         "Type": "ConstitutiveMatrixCartesian", 
         "Swarm": "picIntegrationPoints", 
         "StiffnessMatrix": "k_matrix"
      }, 
      "g_matrix": {
         "Type": "StiffnessMatrix", 
         "RowVariable": "VelocityField", 
         "ColumnVariable": "PressureField", 
         "RHS": "mom_force", 
         "transposeRHS": "cont_force", 
         "allowZeroElementContributions": false
      }, 
      "gradientStiffnessMatrixTerm": {
         "Type": "GradientStiffnessMatrixTerm", 
         "Swarm": "gaussSwarm", 
         "StiffnessMatrix": "g_matrix"
      }, 
      "preconditioner": {
         "Type": "StiffnessMatrix", 
         "RowVariable": "PressureField", 
         "ColumnVariable": "PressureField", 
         "RHS": "cont_force", 
         "allowZeroElementContributions": true
      }, 
      "preconditionerTerm": {
         "Type": "UzawaPreconditionerTerm", 
         "Swarm": "picIntegrationPoints", 
         "StiffnessMatrix": "preconditioner"
      }, 
      "uzawa": {
         "Type": "Stokes_SLE_UzawaSolver", 
         "velocitySolver": "matrixSolver", 
         "Preconditioner": "preconditioner", 
         "tolerance": 1e-05, 
         "monitor": false, 
         "maxIterations": 5000, 
         "minIterations": 1
      }, 
      "stokesEqn": {
         "Type": "Stokes_SLE", 
         "SLE_Solver": "uzawa", 
         "Context": "context", 
         "StressTensorMatrix": "k_matrix", 
         "GradientMatrix": "g_matrix", 
         "DivergenceMatrix": null, 
         "CompressibilityMatrix": "c_matrix", 
         "VelocityVector": "solutionVelocity", 
         "PressureVector": "solutionPressure", 
         "ForceVector": "mom_force", 
         "ContinuityForceVector": "cont_force", 
         "killNonConvergent": false, 
         "nonLinearMaxIterations": "nonLinearMaxIterations", 
         "nonLinearTolerance": "nonLinearTolerance", 
         "makeConvergenceFile": false
      }, 
      "buoyancyForceTerm": {
         "Type": "BuoyancyForceTerm", 
         "ForceVector": "mom_force", 
         "TemperatureField": "TemperatureField", 
         "Swarm": "picIntegrationPoints", 
         "gravity": "gravity"
      }, 
      "context": {
         "Type": "UnderworldContext"
      }, 
      "backgroundViscosity": {
         "Type": "MaterialViscosity", 
         "eta0": 1.0
      }, 
      "backgroundShape": {
         "Type": "Everywhere"
      }, 
      "background": {
         "Type": "RheologyMaterial", 
         "Shape": "backgroundShape", 
         "density": 1.0, 
         "Rheology": "backgroundViscosity"
      }, 
      "lightLayerShape": {
         "Type": "BelowCosinePlane", 
         "offset": 0.2, 
         "amplitude": 0.02, 
         "wavelength": 1.8284, 
         "phase": 0.0
      }, 
      "lightLayerViscosity": {
         "Type": "MaterialViscosity", 
         "eta0": 1.0
      }, 
      "lightLayer": {
         "Type": "RheologyMaterial", 
         "Shape": "lightLayerShape", 
         "density": 0.0, 
         "Rheology": "lightLayerViscosity"
      }, 
      "camera": {
         "Type": "lucCamera", 
         "CentreFieldVariable": "StrainRateInvariantField", 
         "coordZ": 1.5
      }, 
      "densityColourMap": {
         "Type": "lucColourMap", 
         "colours": "Red Blue", 
         "dynamicRange": false
      }, 
      "particleDots": {
         "Type": "lucSwarmViewer", 
         "Swarm": "materialSwarm", 
         "ColourVariable": "materialSwarm-Density", 
         "ColourMap": "densityColourMap", 
         "pointSize": 2.0
      }, 
      "border": {
         "Type": "lucFieldVariableBorder", 
         "FieldVariable": "StrainRateInvariantField"
      }, 
      "densityTitle": {
         "Type": "lucTitle", 
         "string": "Particle Density"
      }, 
      "ParticleDensityVP": {
         "Type": "lucViewport", 
         "Camera": "camera", 
         "DrawingObject": [
            "particleDots", 
            "densityTitle", 
            "border"
         ]
      }, 
      "strainRateInvColourMap": {
         "Type": "lucColourMap", 
         "colours": "Purple Blue Green Yellow Orange Red", 
         "dynamicRange": true
      }, 
      "strainRateInvTitle": {
         "Type": "lucTitle", 
         "string": "Strain Rate Invariant"
      }, 
      "strainRateInvMap": {
         "Type": "lucScalarField", 
         "FieldVariable": "StrainRateInvariantField", 
         "ColourMap": "strainRateInvColourMap"
      }, 
      "strainRateInvColourBar": {
         "Type": "lucColourBar", 
         "ColourMap": "strainRateInvColourMap"
      }, 
      "StrainRateInvariantVP": {
         "Type": "lucViewport", 
         "Camera": "camera", 
         "DrawingObject": [
            "strainRateInvMap", 
            "strainRateInvTitle", 
            "border", 
            "strainRateInvColourBar"
         ]
      }, 
      "database": {
         "Type": "lucDatabase"
      }, 
      "window": {
         "Type": "lucWindow", 
         "Database": "database", 
         "Viewport": "ParticleDensityVP StrainRateInvariantVP"
      }
   }, 
   "velocityMesh": "linearMesh", 
   "elementMesh": "linearMesh", 
   "pressureMesh": "constantMesh", 
   "FieldVariablesToCheckpoint": [
      "VelocityField", 
      "PressureField"
   ], 
   "journal.info.Context-verbose": false, 
   "journal.debug": "f", 
   "journal-level.info.Stg_ComponentFactory": 1, 
   "journal-enable-branch.debug.StgFEM": 1, 
   "journal-level-branch.debug.StgFEM": 1, 
   "journal-enable-branch.debug.Swarm": "f", 
   "journal-level-branch.debug.Swarm": 1, 
   "maxTimeSteps": 10, 
   "dumpEvery": 1, 
   "outputPath": "./output/", 
   "gravity": 1.0, 
   "checkpointEvery": 0, 
   "dim": 2, 
   "elementResI": 64, 
   "elementResJ": 64, 
   "elementResK": 1, 
   "shadowDepth": 1, 
   "minX": 0.0, 
   "minY": 0.0, 
   "minZ": 0.0, 
   "maxX": 0.9142, 
   "maxY": 1.0, 
   "maxZ": 1.0, 
   "allowUnbalancing": true, 
   "buildElementNodeTbl": true, 
   "particleLayoutType": "random", 
   "particlesPerCell": 20, 
   "seed": 13, 
   "velocityBCs": {
      "type": "CompositeVC", 
      "vcList": [
         {
            "type": "WallVC", 
            "wall": "bottom", 
            "variables": [
               {
                  "name": "vx", 
                  "type": "double", 
                  "value": 0
               }, 
               {
                  "name": "vy", 
                  "type": "double", 
                  "value": 0
               }
            ]
         }, 
         {
            "type": "WallVC", 
            "wall": "left", 
            "variables": [
               {
                  "name": "vx", 
                  "type": "double", 
                  "value": 0
               }
            ]
         }, 
         {
            "type": "WallVC", 
            "wall": "right", 
            "variables": [
               {
                  "name": "vx", 
                  "type": "double", 
                  "value": 0
               }
            ]
         }, 
         {
            "type": "WallVC", 
            "wall": "front", 
            "variables": [
               {
                  "name": "vz", 
                  "type": "double", 
                  "value": 0
               }
            ]
         }, 
         {
            "type": "WallVC", 
            "wall": "back", 
            "variables": [
               {
                  "name": "vz", 
                  "type": "double", 
                  "value": 0
               }
            ]
         }, 
         {
            "type": "WallVC", 
            "wall": "top", 
            "variables": [
               {
                  "name": "vx", 
                  "type": "double", 
                  "value": 0
               }, 
               {
                  "name": "vy", 
                  "type": "double", 
                  "value": 0
               }
            ]
         }
      ]
   }

}

